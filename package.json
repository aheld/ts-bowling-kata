  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "reveal-md": "^5.3.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
